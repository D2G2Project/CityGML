target		:cityobject/{"id"} brid:lod2Solid :solidgeometry/{"id"} .
source		SELECT "id", "lod2_solid_id"
			FROM "bridge"

mappingId	Bridge - LoD3Solid
target		:cityobject/{"id"} brid:lod3Solid :solidgeometry/{"id"} .
source		SELECT "id", "lod3_solid_id"
			FROM "bridge"

mappingId	Bridge - LoD4Solid
target		:cityobject/{"id"} brid:lod4Solid :solidgeometry/{"id"} .
source		SELECT "id", "lod4_solid_id"
