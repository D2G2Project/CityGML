[MappingDeclaration] @collection [[
mappingId	Address
target		:address/{"id"} a core:Address ; rdfs:label {"streetaddress"}^^www:XMLSchema#string ; geo:asWKT {multi_point_wkt}^^geo:wktLiteral .
source		SELECT "id", "multi_point", ST_ASTEXT(ST_TRANSFORM(ST_SWAPORDINATES("multi_point", 'xy'), 4326)) AS "multi_point_wkt", "po_box", "state", "street", "zip_code", CONCAT("street", ' ', "house_number") AS "streetaddress"
			FROM "address"

mappingId	Address to xALAddress
target		:address/{"id"} core:hasxalAddress :xaladdress/{"id"} .
source		SELECT "id"
			FROM "address"

mappingId	xALAddress
target		:xaladdress/{"id"} a core:xalAddress ; :xalLocalityName {"xal_locality_name"}^^www:XMLSchema#string ; :xalLocalityType {"xal_locality_type"}^^www:XMLSchema#string ; :xalThoroughfareName {"xal_thoroughfare_name"}^^www:XMLSchema#string ; :xalThoroughfareType {"xal_thoroughfare_type"}^^www:XMLSchema#string .
source		SELECT "id", "xal_locality_name", "xal_locality_type", "xal_thoroughfare_name", "xal_thoroughfare_type"
			FROM "address"

mappingId	Address to Building
target		:cityobject/{"building_id"} bldg:address :address/{"address_id"} .
source		SELECT "address_id", "building_id"
			FROM "address_to_building"

mappingId	Solid Geometry
target		:solidgeometry/{"id"} a sf:PolyhderalSurface ; geo:asWKT {"geometry_wkt"}^^geo:wktLiteral .
source		SELECT "id", ST_ASTEXT(ST_TRANSFORM("solid_geometry", 4326)) AS "geometry_wkt"
			FROM "surface_geometry"
			WHERE "solid_geometry" IS NOT NULL

mappingId	Non-Solid Geometry
target		:geometry/{"id"} a sf:Polygon ; geo:asWKT {"geometry_wkt"}^^geo:wktLiteral ; geo:hasMetricArea {"area_sqm"}^^xsd:double .
source		SELECT "id", ST_ASTEXT(ST_TRANSFORM("geometry", 4326)) AS "geometry_wkt", ST_AREA("geometry") AS "area_sqm"
			FROM "surface_geometry"
			WHERE "geometry" IS NOT NULL

mappingId	Surface Geometry to City Object
target		:cityobject/{"cityobject_id"} geo:hasGeometry :geometry/{"id"} .
source		SELECT "cityobject_id", "id"
			FROM "surface_geometry"
			WHERE "geometry" IS NOT NULL

mappingId	Surface Geometry - Parent
target		:geometry/{"id"} grp:parent :geometry/{"parent_id"} .
source		SELECT "id", "parent_id"
			FROM "surface_geometry"

mappingId	Thematic Surface - Link Building to BuildingSurface LoD2 p1
target		:cityobject/{"building_id"} bldg:boundedBy :cityobject/{"id"} .
source		SELECT building_id, id
			FROM thematic_surface

mappingId	BuildingFloorSurface
target		:cityobject/{"id"} a bldg:WallSurface .
source		SELECT id
			FROM cityobject
			WHERE objectclass_id=32

mappingId	BuildingWallSurface
target		:cityobject/{"id"} a bldg:WallSurface .
source		SELECT id
			FROM cityobject
			WHERE objectclass_id=34

mappingId	BuildingRoofSurface
target		:cityobject/{"id"} a bldg:RoofSurface .
source		SELECT id
			FROM cityobject
			WHERE objectclass_id=33

mappingId	BuildingGroundSurface
target		:cityobject/{"id"} a bldg:GroundSurface .
source		SELECT id
			FROM cityobject
			WHERE objectclass_id=35

mappingId	BuildingClosureSurface
target		:cityobject/{"id"} a bldg:GroundSurface .
source		SELECT id
			FROM cityobject
			WHERE objectclass_id=35

mappingId	Appearance
target		:appearance/{"id"} a app:Appearance ; rdfs:label {"name"}^^www:XMLSchema#string ; app:theme {"theme"}^^www:XMLSchema#string .
source		SELECT "id", "name", "theme"
			FROM "appearance"

mappingId	Simple Texture
target		:texture/{"id"} a tex:SimpleTexture ; app:mimeType {"tex_mime_type"}^^www:XMLSchema#string ; app:imageURI {"tex_image_uri"}^^www:XMLSchema#string .
source		SELECT "id", "tex_image_uri", "tex_mime_type"
			FROM "tex_image"

mappingId	Bridge Opening
target		:cityobject/{"id"} a brid:_Opening .
source		SELECT "id"
			FROM "bridge_opening"

mappingId	Bridge Opening - Address
target		:cityobject/{"id"} brid:address :address/{"address_id"} .
source		SELECT "address_id", "id"
			FROM "bridge_opening"

mappingId	Bridge Opening - LoD3MultiSurface
target		:cityobject/{"id"} brid:lod3MultiSurface :geometry/{"lod3_multi_surface_id"} .
source		SELECT "id", "lod3_multi_surface_id"
			FROM "bridge_opening"

mappingId	Bridge Opening - LoD4MultiSurface
target		:cityobject/{"id"} brid:lod4MultiSurface :geometry/{"lod4_multi_surface_id"} .
source		SELECT "id", "lod4_multi_surface_id"
			FROM "bridge_opening"

mappingId	Bridge Opening - LoD3ImplicitRep
target		:cityobject/{"id"} brid:lod3ImplicitRepresentation :geometry/{"lod3_implicit_rep_id"} .
source		SELECT "id", "lod3_implicit_rep_id"
			FROM "bridge_opening"

mappingId	Bridge Opening - LoD4ImplicitRep
target		:cityobject/{"id"} brid:lod4ImplicitRepresentation :geometry/{"lod4_implicit_rep_id"} .
source		SELECT "id", "lod4_implicit_rep_id"
			FROM "bridge_opening"

mappingId	Bridge Room
target		:bridge_room/{"id"} a brid:BridgeRoom ; brid:class {"class"}^^www:XMLSchema#string ; brid:usage {"usage"}^^www:XMLSchema#string ; brid:function {"function"}^^www:XMLSchema#string .
source		SELECT "bridge_id", "class", "function", "id", "usage"
			FROM "bridge_room"

mappingId	Bridge Room - LoD4MultiSurface
target		:bridge_room/{"id"} brid:lod4MultiSurface :geometry/{"lod4_multi_surface_id"} .
source		SELECT "id", "lod4_multi_surface_id"
			FROM "bridge_room"

mappingId	Bridge Room - Interior
target		:cityobject/{"bridge_id"} brid:interiorBridgeRoom :bridge_room/{"id"} .
source		SELECT "bridge_id", "id"
			FROM "bridge_room"

mappingId	Bridge Room - LoD4Solid
target		:bridge_room/{"id"} brid:lod4Solid :solidgeometry/{"lod4_solid_id"} .
source		SELECT "id", "lod4_solid_id"
			FROM "bridge_room"

mappingId	Transportation Complex - LoD1MultiSurface
target		:transportation_complex/{"id"} tran:lod1MultiSurface :geometry/{"lod1_multi_surface_id"} .
source		SELECT "id", "lod1_multi_surface_id"
			FROM "transportation_complex"

mappingId	Transportation Complex - LoD2MultiSurface
target		:transportation_complex/{"id"} tran:lod2MultiSurface :geometry/{"lod2_multi_surface_id"} .
source		SELECT "id", "lod2_multi_surface_id"
