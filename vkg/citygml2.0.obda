; Appearance and Texture

mappingId	Appearance
target		:appearance/{"id"} a app:Appearance ; rdfs:label {"name"}^^xsd:string ; app:theme {"theme"}^^xsd:string .
source		SELECT "id", "name", "theme"
			FROM "appearance"

mappingId	Simple Texture
target		:texture/{"id"} a tex:SimpleTexture ; app:mimeType {"tex_mime_type"}^^xsd:string ; app:imageURI {"tex_image_uri"}^^xsd:string .
source		SELECT "id", "tex_image_uri", "tex_mime_type"
			FROM "tex_image"

mappingId	Surface Data
target		:surface_data/{"id"} a app:_SurfaceData ; app:specularColor {"x3d_specular_color"}^^xsd:string ; app:preferWorldFile {"gt_prefer_worldfile"}^^xsd:boolean ; app:isFront {"is_front"}^^xsd:boolean ; app:ambientIntensity {"x3d_ambient_intensity"}^^xsd:decimal ; app:shininess {"x3d_shininess"}^^xsd:decimal ; app:borderColor {"tex_border_color"}^^xsd:string ; app:isSmooth {"x3d_is_smooth"}^^xsd:boolean ; app:orientation {"gt_orientation"}^^xsd:string ; app:emissiveColor {"x3d_emissive_color"}^^xsd:string ; rdfs:label {"name"}^^xsd:string ; app:textureType {"tex_texture_type"}^^xsd:string ; app:diffuseColor {"x3d_diffuse_color"}^^xsd:string ; app:transparency {"x3d_transparency"}^^xsd:decimal ; geo:asWKT {"gt_reference_point_wkt"}^^geo:wktLiteral .
source		SELECT "description", "gmlid", "gt_orientation", "gt_prefer_worldfile", ST_ASTEXT(ST_TRANSFORM(ST_SWAPORDINATES("gt_reference_point", 'xy'), 4326)) AS "gt_reference_point_wkt", "id", "is_front", "name", "objectclass_id", "tex_border_color", "tex_image_id", "tex_texture_type", "tex_wrap_mode", "x3d_ambient_intensity", "x3d_diffuse_color", "x3d_emissive_color", "x3d_is_smooth", "x3d_shininess", "x3d_specular_color", "x3d_transparency"
			FROM "surface_data"

mappingId	Texture Parametrization
target		:textureparam/{"surface_geometry_id"} a app:_TextureParameterization ; app:worldToTexture {"world_to_texture"}^^xsd:string ; app:surfaceDataMember :surface_data/{"surface_data_id"} ; tex:textureCoordinates {"texture_coordinates_wkt"}^^geo:wktLiteral .
source		SELECT "is_texture_parametrization", "surface_data_id", "surface_geometry_id", ST_ASTEXT(ST_TRANSFORM(ST_SWAPORDINATES("texture_coordinates", 'xy'), 4326)) AS "texture_coordinates_wkt", "world_to_texture"
			FROM "textureparam"

; Bridge

mappingId	Bridge
target		:cityobject/{"id"} a brid:Bridge ; brid:isMovable {"is_movable"}^^xsd:decimal ; brid:function {"function"}^^xsd:string ; tun:yearOfConstruction {"year_of_construction"}^^xsd:date ; brid:usage {"usage"}^^xsd:string ; brid:class {"class"}^^xsd:string ; brid:yearOfDemolition {"year_of_demolition"}^^xsd:date .
source		SELECT "bridge_parent_id", "class", "function", "id", "is_movable", "usage", "year_of_construction", "year_of_demolition"
			FROM "bridge"

mappingId	Bridge - LoD1MultiSurface
target		:cityobject/{"id"} brid:lod3MultiSurface :geometry/{"lod1_multi_surface_id"} .
source		SELECT "id", "lod1_multi_surface_id"
			FROM "bridge"

