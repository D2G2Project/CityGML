mappingId	Transportation Complex
target		:transportation_complex/{"id"} a tran:TransportationComplex ; tran:function {"function"}^^www:XMLSchema#string ; tran:class {"class"}^^www:XMLSchema#string ; tran:usage {"usage"}^^www:XMLSchema#string .
source		SELECT "class", "function", "id", "usage"
			FROM "transportation_complex"

mappingId	Plant Cover
target		:cityobject/{"id"} a veg:PlantCover ; veg:class {"class"}^^www:XMLSchema#string ; veg:averageHeight {"average_height"}^^www:XMLSchema#decimal ; veg:function {"function"}^^www:XMLSchema#string ; veg:usage {"usage"}^^www:XMLSchema#string .
source		SELECT "average_height", "class", "function", "id", "usage"
