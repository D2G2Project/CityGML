mappingId	Address to bridge
target		:cityobject/{"bridge_id"} brid:address :address/{"address_id"} .
source		SELECT "address_id", "bridge_id"
			FROM "address_to_bridge"

mappingId	Bridge Furniture
target		:cityobject/{"id"} a brid:BridgeFurniture ; brid:class {"class"}^^xsd:string ; brid:usage {"usage"}^^xsd:string ; brid:function {"function"}^^xsd:string .
source		SELECT "bridge_room_id", "class", "function", "id", "lod4_brep_id", "lod4_implicit_ref_point", "lod4_implicit_rep_id", "lod4_implicit_transformation", "lod4_other_geom", "objectclass_id", "usage"
