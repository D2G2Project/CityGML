mappingId	Building Installation - LoD4ImplicitRep
target		:building_installation/{"id"} bldg:lod4ImplicitRepresentation :geometry/{"lod4_implicit_rep_id"} .
source		SELECT "id", "lod4_implicit_rep_id"
			FROM "building_installation"

mappingId	Implicit Geometry
target		:geometry/{"id"} a core:ImplicitGeometry ; core:libraryObject {"library_object"}^^www:XMLSchema#hexBinary ; core:mimeType {"mime_type"}^^www:XMLSchema#string .
source		SELECT "id", "library_object", "mime_type", "reference_to_library", "relative_brep_id", "relative_other_geom"
			FROM "implicit_geometry"

mappingId	Relief Feature to Relative Component
target		:cityobject/{"relief_feature_id"} dem:reliefComponent :relief_component/{"relief_component_id"} .
source		SELECT "relief_component_id", "relief_feature_id"
			FROM "relief_feat_to_rel_comp"

mappingId	gml Object
target		:object/{"id"} a gml:_Object ; grp:class {"classname"}^^www:XMLSchema#string .
source		SELECT "classname", "id"
			FROM "objectclass"

mappingId	Generic City Object
target		:genericcityobject/{"id"} a gen:GenericCityObject ; gen:usage {"usage"}^^www:XMLSchema#string ; gen:class {"class"}^^www:XMLSchema#string ; gen:function {"function"}^^www:XMLSchema#string .
source		SELECT "class", "function", "id", "lod0_brep_id", "lod0_implicit_ref_point", "lod0_implicit_rep_id", "lod0_implicit_transformation", "lod0_other_geom", "lod0_terrain_intersection", "lod1_brep_id", "lod1_implicit_ref_point", "lod1_implicit_rep_id", "lod1_implicit_transformation", "lod1_other_geom", "lod1_terrain_intersection", "lod2_brep_id", "lod2_implicit_ref_point", "lod2_implicit_rep_id", "lod2_implicit_transformation", "lod2_other_geom", "lod2_terrain_intersection", "lod3_brep_id", "lod3_implicit_ref_point", "lod3_implicit_rep_id", "lod3_implicit_transformation", "lod3_other_geom", "lod3_terrain_intersection", "lod4_brep_id", "lod4_implicit_ref_point", "lod4_implicit_rep_id", "lod4_implicit_transformation", "lod4_other_geom", "lod4_terrain_intersection", "objectclass_id", "usage"
			FROM "generic_cityobject"

mappingId	Generic City Object - LoD0ImplicitRep
target		:genericcityobject/{"id"} gen:lod0ImplicitRepresentation :geometry/{"lod0_implicit_rep_id"} .
source		SELECT "id", "lod0_implicit_rep_id"
			FROM "generic_cityobject"

mappingId	Generic City Object - LoD1ImplicitRep
target		:genericcityobject/{"id"} gen:lod1ImplicitRepresentation :geometry/{"lod1_implicit_rep_id"} .
source		SELECT "id", "lod1_implicit_rep_id"
			FROM "generic_cityobject"

mappingId	Generic City Object - LoD2ImplicitRep
target		:genericcityobject/{"id"} gen:lod2ImplicitRepresentation :geometry/{"lod2_implicit_rep_id"} .
source		SELECT "id", "lod2_implicit_rep_id"
			FROM "generic_cityobject"

mappingId	Generic City Object - LoD3ImplicitRep
target		:genericcityobject/{"id"} gen:lod3ImplicitRepresentation :geometry/{"lod3_implicit_rep_id"} .
