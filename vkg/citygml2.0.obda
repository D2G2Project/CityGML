source		SELECT "id", "lod4_implicit_rep_id"
			FROM "generic_cityobject"

mappingId	Bridge Furniture
target		:cityobject/{"id"} a brid:BridgeFurniture ; brid:class {"class"}^^www:XMLSchema#string ; brid:usage {"usage"}^^www:XMLSchema#string ; brid:function {"function"}^^www:XMLSchema#string .
source		SELECT "bridge_room_id", "class", "function", "id", "lod4_brep_id", "lod4_implicit_ref_point", "lod4_implicit_rep_id", "lod4_implicit_transformation", "lod4_other_geom", "objectclass_id", "usage"
