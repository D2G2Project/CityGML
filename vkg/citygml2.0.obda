[MappingDeclaration] @collection [[

; Addresses

mappingId	Address
target		:address/{"id"} a core:Address ; rdfs:label {"streetaddress"}^^xsd:string ; geo:asWKT {multi_point_wkt}^^geo:wktLiteral .
source		SELECT "id", "multi_point", ST_ASTEXT(ST_TRANSFORM(ST_SWAPORDINATES("multi_point", 'xy'), 4326)) AS "multi_point_wkt", "po_box", "state", "street", "zip_code", CONCAT("street", ' ', "house_number") AS "streetaddress"
			FROM "address"

mappingId	Address to xALAddress
target		:address/{"id"} core:hasxalAddress :xaladdress/{"id"} .
source		SELECT "id"
			FROM "address"

mappingId	xALAddress
target		:xaladdress/{"id"} a core:xalAddress ; :xalLocalityName {"xal_locality_name"}^^xsd:string ; :xalLocalityType {"xal_locality_type"}^^xsd:string ; :xalThoroughfareName {"xal_thoroughfare_name"}^^xsd:string ; :xalThoroughfareType {"xal_thoroughfare_type"}^^xsd:string .
source		SELECT "id", "xal_locality_name", "xal_locality_type", "xal_thoroughfare_name", "xal_thoroughfare_type"
			FROM "address"

; Geometries and Thematic Surfaces

mappingId	Solid Geometry
target		:solidgeometry/{"id"} a sf:PolyhderalSurface ; geo:asWKT {"geometry_wkt"}^^geo:wktLiteral .
source		SELECT "id", ST_ASTEXT(ST_TRANSFORM("solid_geometry", 4326)) AS "geometry_wkt"
			FROM "surface_geometry"
			WHERE "solid_geometry" IS NOT NULL

mappingId	Non-Solid Geometry
target		:geometry/{"id"} a sf:Polygon ; geo:asWKT {"geometry_wkt"}^^geo:wktLiteral ; geo:hasMetricArea {"area_sqm"}^^xsd:double .
source		SELECT "id", ST_ASTEXT(ST_TRANSFORM("geometry", 4326)) AS "geometry_wkt", ST_AREA("geometry") AS "area_sqm"
			FROM "surface_geometry"
			WHERE "geometry" IS NOT NULL

mappingId	Surface Geometry to City Object
target		:cityobject/{"cityobject_id"} geo:hasGeometry :geometry/{"id"} .
source		SELECT "cityobject_id", "id"
			FROM "surface_geometry"
			WHERE "geometry" IS NOT NULL

mappingId	Surface Geometry - Parent
target		:geometry/{"id"} grp:parent :geometry/{"parent_id"} .
source		SELECT "id", "parent_id"
			FROM "surface_geometry"

mappingId	Thematic Surface - Link Building to BuildingSurface LoD2 p1
target		:cityobject/{"building_id"} bldg:boundedBy :cityobject/{"id"} .
source		SELECT building_id, id
			FROM thematic_surface

; Building

mappingId	Building
target		:cityobject/{"id"} a bldg:Building ; bldg:storeysBelowGround {"storeys_below_ground"}^^xsd:decimal ; bldg:storeysAboveGround {"storeys_above_ground"}^^xsd:decimal ; bldg:function {"function"}^^xsd:string ; bldg:usage {"usage"}^^xsd:string ; bldg:storeyHeightsAboveGround {"storey_heights_above_ground"}^^xsd:decimal ; bldg:yearOfConstruction {"year_of_construction"}^^xsd:date ; bldg:yearOfDemolition {"year_of_demolition"}^^xsd:date ; bldg:measuredHeight {"measured_height"}^^xsd:double ; bldg:storeyHeightsBelowGround {"storey_heights_below_ground"}^^xsd:decimal ; bldg:class {"class"}^^xsd:string .
source		SELECT "building_parent_id", "building_root_id", "class", "function", "id", "measured_height", "measured_height_unit", "storey_heights_above_ground", "storey_heights_below_ground", "storeys_above_ground", "storeys_below_ground", "usage", "year_of_construction", "year_of_demolition"
			FROM "building"

mappingId	Address to Building
target		:cityobject/{"building_id"} bldg:address :address/{"address_id"} .
source		SELECT "address_id", "building_id"
			FROM "address_to_building"

mappingId	Building_Roof_Type
target		:cityobject/{"id"} bldg:roofSurface :roof/{"roof_type"} .
source		SELECT "id", "roof_type"
			FROM "building"

mappingId	Building_Roof_Type_Name
target		:roof/{"name"} bldg:roofType {"description_en"}^^xsd:string .
source		SELECT "name", "description_en"
			FROM "roof_codelist"

mappingId	Building - LoD1MultiSurface
target		:cityobject/{"id"} bldg:lod1MultiSurface :geometry/{"lod1_multi_surface_id"} .
source		SELECT "id", "lod1_multi_surface_id"
			FROM "building"

mappingId	Building - LoD2MultiSurface
target		:cityobject/{"id"} bldg:lod2MultiSurface :geometry/{"lod2_multi_surface_id"} .
source		SELECT "id", "lod2_multi_surface_id"
			FROM "building"

mappingId	Building - LoD3MultiSurface
target		:cityobject/{"id"} bldg:lod3MultiSurface :geometry/{"lod3_multi_surface_id"} .
source		SELECT "id", "lod3_multi_surface_id"
			FROM "building"

mappingId	Building - LoD4MultiSurface
target		:cityobject/{"id"} bldg:lod4MultiSurface :geometry/{"lod4_multi_surface_id"} .
source		SELECT "id", "lod4_multi_surface_id"
			FROM "building"

mappingId	Building - LoD1Solid
target		:cityobject/{"id"} bldg:lod1Solid :solidgeometry/{"lod1_solid_id"} .
source		SELECT "id", "lod1_solid_id"
			FROM "building"

mappingId	Building - LoD2Solid
target		:cityobject/{"id"} bldg:lod2Solid :solidgeometry/{"lod2_solid_id"} .
source		SELECT "id", "lod2_solid_id"
			FROM "building"

mappingId	Building - LoD3Solid
target		:cityobject/{"id"} bldg:lod3Solid :solidgeometry/{"lod3_solid_id"} .
source		SELECT "id", "lod3_solid_id"
			FROM "building"

mappingId	Building - LoD4Solid
target		:cityobject/{"id"} bldg:lod4Solid :solidgeometry/{"lod4_solid_id"} .
source		SELECT "id", "lod4_solid_id"
			FROM "building"

mappingId	Building - LoDRoofprint
target		:cityobject/{"id"} bldg:lod0RoofEdge :geometry/{"lod0_roofprint_id"} .
source		SELECT "id", "lod0_roofprint_id"
			FROM "building"

mappingId	Building - LoDFootprint
target		:cityobject/{"id"} bldg:lod0FootPrint :geometry/{"lod0_footprint_id"} .
source		SELECT "id", "lod0_footprint_id"
			FROM "building"

mappingId	Building - Parent
target		:cityobject/{"building_parent_id"} grp:parent :cityobject/{"id"} .
source		SELECT "building_parent_id", "id"
			FROM "building"

mappingId	BuildingFloorSurface
target		:cityobject/{"id"} a bldg:WallSurface .
source		SELECT id
			FROM cityobject
			WHERE objectclass_id=32

mappingId	BuildingWallSurface
target		:cityobject/{"id"} a bldg:WallSurface .
source		SELECT id
			FROM cityobject
			WHERE objectclass_id=34

mappingId	BuildingRoofSurface
target		:cityobject/{"id"} a bldg:RoofSurface .
source		SELECT id
			FROM cityobject
			WHERE objectclass_id=33

mappingId	BuildingGroundSurface
target		:cityobject/{"id"} a bldg:GroundSurface .
source		SELECT id
			FROM cityobject
			WHERE objectclass_id=35

mappingId	BuildingClosureSurface
target		:cityobject/{"id"} a bldg:GroundSurface .
source		SELECT id
			FROM cityobject
			WHERE objectclass_id=35

mappingId	Building Installation
target		:building_installation/{"id"} a bldg:BuildingInstallation ; bldg:function {"function"}^^xsd:string ; bldg:class {"class"}^^xsd:string ; bldg:usage {"usage"}^^xsd:string .
source		SELECT "building_id", "class", "function", "id", "lod2_brep_id", "lod2_implicit_ref_point", "lod2_implicit_rep_id", "lod2_implicit_transformation", "lod2_other_geom", "lod3_brep_id", "lod3_implicit_ref_point", "lod3_implicit_rep_id", "lod3_implicit_transformation", "lod3_other_geom", "lod4_brep_id", "lod4_implicit_ref_point", "lod4_implicit_rep_id", "lod4_implicit_transformation", "lod4_other_geom", "objectclass_id", "room_id", "usage"
			FROM "building_installation"

mappingId	Building Installation - Room Installation
target		:building_installation/{"id"} bldg:roomInstallation :room/{"room_id"} .
source		SELECT "id", "room_id"
			FROM "building_installation"

mappingId	Building Installation - LoD2ImplicitRep
target		:building_installation/{"id"} bldg:lod2ImplicitRepresentation :geometry/{"lod2_implicit_rep_id"} .
source		SELECT "id", "lod2_implicit_rep_id"
			FROM "building_installation"

mappingId	Building Installation - LoD3ImplicitRep
target		:building_installation/{"id"} bldg:lod3ImplicitRepresentation :geometry/{"lod3_implicit_rep_id"} .
source		SELECT "id", "lod3_implicit_rep_id"
			FROM "building_installation"

mappingId	Building Installation - LoD4ImplicitRep
target		:building_installation/{"id"} bldg:lod4ImplicitRepresentation :geometry/{"lod4_implicit_rep_id"} .
source		SELECT "id", "lod4_implicit_rep_id"
			FROM "building_installation"

mappingId	Building Furniture
target		:cityobject/{"id"} a bldg:BuildingFurniture ; bldg:class {"class"}^^xsd:string ; bldg:usage {"usage"}^^xsd:string ; bldg:function {"function"}^^xsd:string .
source		SELECT "class", "function", "id", "lod4_brep_id", "lod4_implicit_ref_point", "lod4_implicit_rep_id", "lod4_implicit_transformation", "lod4_other_geom", "objectclass_id", "room_id", "usage"
			FROM "building_furniture"

mappingId	Building Furniture - LoD4ImplicitRep
target		:cityobject/{"id"} bldg:lod4ImplicitRepresentation :geometry/{"lod4_implicit_rep_id"} .
source		SELECT "id", "lod4_implicit_rep_id"
			FROM "building_furniture"

mappingId	Building Furniture - Interior Room
target		:cityobject/{"id"} bldg:interiorRoom :room/{"room_id"} .
source		SELECT "id", "room_id"
			FROM "building_furniture"

; Room

mappingId	Room
target		:cityobject/{"id"} a bldg:Room ; bldg:class {"class"}^^xsd:string ; bldg:function {"function"}^^xsd:string ; bldg:usage {"usage"}^^xsd:string .
source		SELECT "class", "function", "id", "usage"
			FROM "room"

mappingId	Room - LoD4Solid
target		:cityobject/{"id"} bldg:lod4Solid :solidgeometry/{"lod4_solid_id"} .
source		SELECT "id", "lod4_solid_id"
