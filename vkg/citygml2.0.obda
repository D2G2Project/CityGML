mappingId	Plant Cover - LoD1MultiSolid
target		:cityobject/{"id"} veg:lod1MultiSolid :solidgeometry/{"lod1_multi_solid_id"} .
source		SELECT "id", "lod1_multi_solid_id"
			FROM "plant_cover"

mappingId	Plant Cover - LoD2MultiSolid
target		:cityobject/{"id"} veg:lod2MultiSolid :solidgeometry/{"lod2_multi_solid_id"} .
source		SELECT "id", "lod2_multi_solid_id"
			FROM "plant_cover"

mappingId	Plant Cover - LoD3MultiSolid
target		:cityobject/{"id"} veg:lod3MultiSolid :solidgeometry/{"lod3_multi_solid_id"} .
source		SELECT "id", "lod3_multi_solid_id"
			FROM "plant_cover"

mappingId	Plant Cover - LoD4MultiSolid
target		:cityobject/{"id"} veg:lod4MultiSolid :solidgeometry/{"lod4_multi_solid_id"} .
source		SELECT "id", "lod4_multi_solid_id"
			FROM "plant_cover"

mappingId	Masspoint Relief
target		:masspoint_relief/{"id"} a dem:MassPointRelief .
source		SELECT "id", "objectclass_id", "relief_points"
			FROM "masspoint_relief"

mappingId	Building Installation
target		:building_installation/{"id"} a bldg:BuildingInstallation ; bldg:function {"function"}^^www:XMLSchema#string ; bldg:class {"class"}^^www:XMLSchema#string ; bldg:usage {"usage"}^^www:XMLSchema#string .
source		SELECT "building_id", "class", "function", "id", "lod2_brep_id", "lod2_implicit_ref_point", "lod2_implicit_rep_id", "lod2_implicit_transformation", "lod2_other_geom", "lod3_brep_id", "lod3_implicit_ref_point", "lod3_implicit_rep_id", "lod3_implicit_transformation", "lod3_other_geom", "lod4_brep_id", "lod4_implicit_ref_point", "lod4_implicit_rep_id", "lod4_implicit_transformation", "lod4_other_geom", "objectclass_id", "room_id", "usage"
			FROM "building_installation"

mappingId	Building Installation - Room Installation
target		:building_installation/{"id"} bldg:roomInstallation :room/{"room_id"} .
source		SELECT "id", "room_id"
